<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="socialnetwork.controller.NewUserController">
    <HBox alignment="CENTER" prefHeight="400" prefWidth="400">
        <ImageView fitHeight="200.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/L2cq.gif" />
            </image>
            <HBox.margin>
                <Insets left="10.0" />
            </HBox.margin>
        </ImageView>
        <VBox prefHeight="400.0" prefWidth="203.0">
           <Label />
            <Label text="FIRSTNAME" />
            <TextField fx:id="fName" />
            <Label />
            <Label text="LASTNAME" />
            <TextField fx:id="lName" />
            <Label />
            <Label text="USERNAME" />
            <TextField fx:id="uName" />
            <Label />
            <Label text="PASSWORD" />
            <TextField fx:id="pass" />
            <Label />
            <Button fx:id="btn" onAction="#hanndleAddUser" text="ADD NEW USER" />
        </VBox>
    </HBox>

</AnchorPane>
